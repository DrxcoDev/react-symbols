{
  "private": true,
  "workspaces": [
    "website",
    "library"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write .",
    "lint": "turbo run lint",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build --filter=website^... && changeset publish"
  },
  "dependencies": {
    "@changesets/cli": "2.25.0",
    "@react-symbols/icons": "*"
  },
  "devDependencies": {
    "prettier": "2.7.1",
    "turbo": "1.6.3"
    "turbo": "1.5.5"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}
